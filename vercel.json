{
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/dist/(.*)",
      "dest": "/public/dist/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    }
  ]
}
